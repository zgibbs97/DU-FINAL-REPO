le.error(e);\n      }\n    },\n  });\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n\n    try {\n      const { data } = addProfile({\n        variables: { name },\n      });\n\n      setName('');\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  return (\n    <div>\n      <h3>Add yourself to the list...</h3>\n      <form\n        className=\"flex-row justify-center justify-space-between-md align-center\"\n        onSubmit={handleFormSubmit}\n      >\n        <div className=\"col-12 col-lg-9\">\n          <input\n            placeholder=\"Add your profile name...\"\n            value={name}\n            className=\"form-input w-100\"\n            onChange={(event) => setName(event.target.value)}\n          />\n        </div>\n\n        <div className=\"col-12 col-lg-3\">\n          <button className=\"btn btn-info btn-block py-3\" type=\"submit\">\n            Add Profile\n          </button>\n        </div>\n        {error && (\n          <div className=\"col-12 my-3 bg-danger text-white p-3\">\n            Something went wrong...\n          </div>\n        )}\n      </form>\n    </div>\n  );\n};\n\nexport default ProfileForm;\n","/Users/jaymascarenas/Dropbox/coding-boot-camp/FullStack-Ground/01-Class-Content/21-MERN/01-Activities/17-Ins_Apollo-Cache/client/src/utils/mutations.js",[],{"ruleId":"44","severity":1,"message":"45","line":34,"column":15,"nodeType":"46","messageId":"47","endLine":34,"endColumn":19},"no-unused-vars","'data' is assigned a value but never used.","Identifier","unusedVar"]